{
  "name": "TransactionService",
  "stateMachine": {
    "created": ["planned","voided"],
    "planned": ["posted","voided"],
    "posted": ["reconciled","voided"],
    "reconciled": [],
    "voided": []
  },
  "editEffects": {
    "planned": "reproject from tx.date",
    "posted": "adjust account delta and reproject from tx.date"
  },
  "deleteRules": {
    "planned": "hard delete",
    "posted": "soft delete with reversal; audit entry"
  }
}